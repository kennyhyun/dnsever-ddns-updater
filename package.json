{
  "name": "dnsever-ddns-updater",
  "preferGlobal": "true",
  "description": "Nodejs Dynamic DNS Updater For DNSEver.com(DNS Service in Korea) User",
  "version": "2.3.1",
  "main": "core.js",
  "license": "MIT",
  "author": "Alfred UC",
  "repository": {
    "type": "git",
    "url": "https://github.com/b6pzeusbc54tvhw5jgpyw8pwz2x6gs/dnsever-ddns-updater"
  },
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "start": "bin/dnsever-ddns-updater show && docker-compose up -d",
